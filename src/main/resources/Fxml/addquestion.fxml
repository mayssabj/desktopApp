<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="Fxml.Addquestion">
    <children>
        <Text fill="#999233" fontSmoothingType="LCD" layoutX="215.0" layoutY="64.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Ask a Question" textAlignment="CENTER" wrappingWidth="369.0">
            <font>
                <Font name="System Bold Italic" size="28.0" />
            </font>
        </Text>
        <TextField fx:id="Qtitle" layoutX="191.0" layoutY="126.0" promptText="Enter title" />
        <TextArea fx:id="Qbody" layoutX="191.0" layoutY="172.0" prefHeight="126.0" prefWidth="389.0" promptText="Enter question body" />
        <Button fx:id="btnask" layoutX="321.0" layoutY="322.0" mnemonicParsing="false" text="Ask" onAction="#addQuestion" />
    </children>
</AnchorPane>
