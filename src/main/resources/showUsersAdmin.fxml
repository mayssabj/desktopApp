<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Pagination?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="700.0" prefWidth="1050.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.esprit.controller.user.AdminController">
    <VBox layoutX="30.0" layoutY="30.0" prefHeight="111.0" prefWidth="1050.0" spacing="10">
        <children>
            <VBox fx:id="tableContainer" prefHeight="30.0" prefWidth="1050.0">
                <!-- The table header and data rows will be dynamically added here -->
            </VBox>
            <Pagination fx:id="pagination" pageCount="1" prefHeight="30.0" prefWidth="1050.0" />
        </children>
    </VBox>
</AnchorPane>
